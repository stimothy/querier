@{
    ViewData["Title"] = "Guest Access";
}
<div class="container-fluid">
    <h2>@ViewData["Title"]</h2>
    <form class="navbar-form no-margin no-padding" role="search">
        <button type="button" class="btn btn-primary">Add Query</button>
        <div class="form-group">
            <input type="text" class="form-control" placeholder="Search">
        </div>
    </form>
</div>

<div class="container-fluid">
    <table class="table table-fixed">
        <thead>
            <tr>
                <th class="col-xs-3">Quiz Name</th>
                <th class="col-xs-3">Description</th>
                <th class="col-xs-6">Options</th>                    
            </tr>
        </thead>
        <tbody>
            <tr>
                <td class="col-xs-3">Query 1</td>
                <td class="col-xs-3">This is a test query. </td>
                <td class="col-xs-6">
                    <button type="button" class="btn btn-info">Edit</button>
                    <button type="button" class="btn btn-success">Publish</button>
                    <button type="button" class="btn btn-danger">Delete</button>
                </td>
            </tr>
            <tr>
                <td class="col-xs-3">Query 2</td>
                <td class="col-xs-3">This is another test query.</td>
                <td class="col-xs-6">
                    <button type="button" class="btn btn-info">Edit</button>
                    <button type="button" class="btn btn-success">Publish</button>
                    <button type="button" class="btn btn-danger">Delete</button>
                </td>
            </tr>
        </tbody>
    </table>
</div>